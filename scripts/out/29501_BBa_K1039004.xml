    <dcterms:title>BBa_K1039004</dcterms:title>
    <dcterms:description>Bxb1 Invertible Promoter Switch (Promoter J23119) (RL State)</dcterms:description>
    <prov:wasGeneratedBy rdf:resource="https://synbiohub.org/public/igem/igem2sbol/1"/>
