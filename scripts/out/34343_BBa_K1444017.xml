    <dcterms:title>LacZ</dcterms:title>
    <dcterms:description>Full length beta-galactosidase (LacZ)</dcterms:description>
    <rdfs:seeAlso rdf:resource="http://www.uniprot.org/uniprot/P00722"/>
    <prov:wasGeneratedBy rdf:resource="https://synbiohub.org/public/igem/igem2sbol/1"/>
