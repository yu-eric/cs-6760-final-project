    <dcterms:description>Temperature Sensitive Lambda cI Repressor</dcterms:description>
    <rdfs:seeAlso rdf:resource="http://www.uniprot.org/uniprot/P03034"/>
    <prov:wasGeneratedBy rdf:resource="https://synbiohub.org/public/igem/igem2sbol/1"/>
