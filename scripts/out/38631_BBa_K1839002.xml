    <dcterms:title>BBa_K1839002</dcterms:title>
    <dcterms:description>Ava_3856 ATP-grasp ligase</dcterms:description>
    <prov:wasGeneratedBy rdf:resource="https://synbiohub.org/public/igem/igem2sbol/1"/>
