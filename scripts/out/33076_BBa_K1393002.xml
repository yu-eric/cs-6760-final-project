    <dcterms:title>BBa_K1393002</dcterms:title>
    <dcterms:description>cyn operon in Escherichia coli BL21(DE3) (cynR+cynT+cynS)</dcterms:description>
    <prov:wasGeneratedBy rdf:resource="https://synbiohub.org/public/igem/igem2sbol/1"/>
