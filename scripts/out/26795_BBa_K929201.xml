    <dcterms:title>BBa_K929201</dcterms:title>
    <dcterms:description>N-terminal sortase motif linked to AAV2-VP2 with kozak and CMV</dcterms:description>
    <prov:wasGeneratedBy rdf:resource="https://synbiohub.org/public/igem/igem2sbol/1"/>
