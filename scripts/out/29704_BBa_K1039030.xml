    <dcterms:title>BBa_K1039030</dcterms:title>
    <dcterms:description>PhiC31 integrase with Promoter and RBS</dcterms:description>
    <prov:wasGeneratedBy rdf:resource="https://synbiohub.org/public/igem/igem2sbol/1"/>
