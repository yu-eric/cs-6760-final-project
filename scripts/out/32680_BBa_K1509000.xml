    <dcterms:description>Coding for the trans-acting regulator SmtB</dcterms:description>
    <rdfs:seeAlso rdf:resource="http://www.uniprot.org/uniprot/P30340"/>
    <prov:wasGeneratedBy rdf:resource="https://synbiohub.org/public/igem/igem2sbol/1"/>
