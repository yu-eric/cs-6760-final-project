    <dcterms:description>OmpA_transmembrane_domain + Linker + point_mutant_HPLC6(AFP)</dcterms:description>
    <prov:wasGeneratedBy rdf:resource="https://synbiohub.org/public/igem/igem2sbol/1"/>
