    <dcterms:description>Mambalgin-1 for E. coli</dcterms:description>
    <prov:wasGeneratedBy rdf:resource="https://synbiohub.org/public/igem/igem2sbol/1"/>
    <sbh:ownedBy rdf:resource="https://synbiohub.org/user/myers"/>
