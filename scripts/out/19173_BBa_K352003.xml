    <dcterms:title>BBa_K352003</dcterms:title>
    <dcterms:description>pCooM Promoter from Rhodospirillum rubrum</dcterms:description>
    <prov:wasGeneratedBy rdf:resource="https://synbiohub.org/public/igem/igem2sbol/1"/>
