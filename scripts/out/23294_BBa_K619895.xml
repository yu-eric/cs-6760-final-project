    <dcterms:title>BBa_K619895</dcterms:title>
    <dcterms:description>mutated prfA-UTR thermosensor from Listeria monocytogenes</dcterms:description>
    <prov:wasGeneratedBy rdf:resource="https://synbiohub.org/public/igem/igem2sbol/1"/>
    <sbh:ownedBy rdf:resource="https://synbiohub.org/user/myers"/>
