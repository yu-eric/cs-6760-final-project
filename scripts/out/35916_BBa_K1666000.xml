    <dcterms:description>LsrA of LuxS/AI-2 signaling pathway in Salmonalla</dcterms:description>
    <rdfs:seeAlso rdf:resource="http://www.uniprot.org/uniprot/Q8ZKQ4"/>
    <prov:wasGeneratedBy rdf:resource="https://synbiohub.org/public/igem/igem2sbol/1"/>
