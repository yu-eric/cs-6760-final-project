    <dcterms:title>pSB1AK3X</dcterms:title>
    <dcterms:description>pSB1AK3X Freiburg Fusion -&gt; BioFusion conversion plasmid</dcterms:description>
    <prov:wasGeneratedBy rdf:resource="https://synbiohub.org/public/igem/igem2sbol/1"/>
