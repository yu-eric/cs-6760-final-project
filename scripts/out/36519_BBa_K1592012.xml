    <dcterms:title>Si-tag1+3</dcterms:title>
    <dcterms:description>LIP prepro + E. coli ribosomal protein L2 (1-60,203-273aa) + YLcwp3 Fusion</dcterms:description>
    <prov:wasGeneratedBy rdf:resource="https://synbiohub.org/public/igem/igem2sbol/1"/>
