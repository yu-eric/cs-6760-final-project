    <dcterms:title>BBa_K174011</dcterms:title>
    <dcterms:description>IPTG inducable kinA Bacillus sporulation trigger</dcterms:description>
    <prov:wasGeneratedBy rdf:resource="https://synbiohub.org/public/igem/igem2sbol/1"/>
