    <dcterms:title>sRBS Pehs</dcterms:title>
    <dcterms:description>Reverse strand coding Enhanced LacI-hyperspank promoter</dcterms:description>
    <prov:wasGeneratedBy rdf:resource="https://synbiohub.org/public/igem/igem2sbol/1"/>
