    <dcterms:title>slipper</dcterms:title>
    <dcterms:description>Protein secretor in Lactobacillus Plantarum</dcterms:description>
    <prov:wasGeneratedBy rdf:resource="https://synbiohub.org/public/igem/igem2sbol/1"/>
