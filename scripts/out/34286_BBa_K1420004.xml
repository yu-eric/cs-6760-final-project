    <dcterms:description>merR family transcriptional regulator, regulatory protein for mer operon</dcterms:description>
    <rdfs:seeAlso rdf:resource="http://www.uniprot.org/uniprot/P13111"/>
    <prov:wasGeneratedBy rdf:resource="https://synbiohub.org/public/igem/igem2sbol/1"/>
