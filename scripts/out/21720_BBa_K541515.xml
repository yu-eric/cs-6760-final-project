    <dcterms:title>BBa_K541515</dcterms:title>
    <dcterms:description>Limulus anti-LPS factor (LALF) for Bacillus subtilis</dcterms:description>
    <prov:wasGeneratedBy rdf:resource="https://synbiohub.org/public/igem/igem2sbol/1"/>
