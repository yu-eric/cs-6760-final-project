    <dcterms:title>BBa_K2008006</dcterms:title>
    <dcterms:description>Xylose-&gt;comK with removal of internal restriction sites</dcterms:description>
    <prov:wasGeneratedBy rdf:resource="https://synbiohub.org/public/igem/igem2sbol/1"/>
