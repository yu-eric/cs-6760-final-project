    <dcterms:title>mdnC</dcterms:title>
    <dcterms:description>ATP-grasp-type ligase from the mdn-cluster</dcterms:description>
    <prov:wasGeneratedBy rdf:resource="https://synbiohub.org/public/igem/igem2sbol/1"/>
