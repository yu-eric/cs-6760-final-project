    <dcterms:title>sinR</dcterms:title>
    <dcterms:description>a luxR-type regulator from Sinorhizobium meliloti 1021</dcterms:description>
    <prov:wasGeneratedBy rdf:resource="https://synbiohub.org/public/igem/igem2sbol/1"/>
