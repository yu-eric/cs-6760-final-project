    <dcterms:title>BBa_K1666004</dcterms:title>
    <dcterms:description>LsrK of LuxS/AI-2 signaling pathway in Salmonalla</dcterms:description>
    <rdfs:seeAlso rdf:resource="http://www.uniprot.org/uniprot/Q8ZKQ6"/>
    <prov:wasGeneratedBy rdf:resource="https://synbiohub.org/public/igem/igem2sbol/1"/>
